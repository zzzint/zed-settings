{
  "assistant": {
    "default_model": {
      "model": "gemini-2.5-pro-exp-03-25",
      "provider": "google"
    },
    "default_profile": "ask",
    "version": "2"
  },
  "buffer_font_family": "JuliaMono",
  "buffer_font_size": 18,
  "buffer_line_height": {
    "custom": 2
  },
  "edit_predictions": {
    "mode": "subtle"
  },
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "git": {
    "git_gutter": "hide",
    "hunk_style": "staged_hollow",
    "inline_blame": {
      "enabled": false
    }
  },
  "gutter": {
    "breakpoints": true,
    "code_actions": false,
    "folds": true,
    "line_numbers": true,
    "runnables": true
  },
  "inlay_hints": {
    "enabled": false
  },
  "max_tabs": 3,
  "relative_line_numbers": true,
  "show_completions_on_input": true,
  "show_edit_predictions": false,
  "terminal": {
    "font_family": "JuliaMono"
  },
  "theme": {
    "dark": "Lydia Dark Experimental",
    "light": "Arrakis Day",
    "mode": "system"
  },
  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": false,
    "selections_menu": true
  },
  "ui_font_family": "JuliaMono",
  "ui_font_size": 18,
  "vim_mode": true
}
